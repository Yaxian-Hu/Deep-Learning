   "metadata": {},
   "outputs": [],
   "source": [
    "def f(a, x):\n",
    "    if x<=0:\n",
    "        b=torch.sqrt(a)\n",
    "    else:\n",
    "        b=torch.square(a)\n",
    "    return b"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "1d0a76dd",
   "metadata": {},
   "outputs": [],
   "source": [
    "a = torch.randn(size=(), requires_grad=True)\n",
    "c = f(a,1)\n",
    "c.backward()"
   ]
  },
